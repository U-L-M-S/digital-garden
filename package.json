  "scripts": {
    "vercel-build": "rm -rf node_modules package-lock.json && npm install && npm run build",
    "prebuild": "rimraf dist",
    "start": "npm-run-all get-theme watch:eleventy",
    "build": "npm-run-all get-theme build:*",
